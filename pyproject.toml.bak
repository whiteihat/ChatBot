[project]
name = "collam"
version = "0.1.0"
description = "collam"
readme = "README.md"
requires-python = ">=3.9, <4.0"

[tool.nonebot]
adapters = [
    { name = "OneBot V11", module_name = "nonebot.adapters.onebot.v11" },
    { name = "QQ", module_name = "nonebot.adapters.qq" },
    { name = "Console", module_name = "nonebot.adapters.console" },
]
plugins = []
plugin_dirs = ["src/plugins"]
builtin_plugins = ["echo"]

[tool.poetry.dependencies]
nonebot-adapter-onebot = "2.4.6"
nonebot-adapter-qq = "1.6.2"
nonebot-adapter-console = "0.6.0"
nonebot2 = { extras = ["fastapi"], version = "^2.4.1" }
openai = "1.65.1"
annotated-types = "0.7.0"
anyio = "4.8.0"
certifi = "2025.1.31"
cffi = "1.17.1"
colorama = "0.4.6"
cryptography = "43.0.3"
distro = "1.9.0"
exceptiongroup = "1.2.2"
h11 = "0.14.0"
httpcore = "1.0.7"
httpx = "0.28.1"
idna = "3.10"
jiter = "0.8.2"
linkify-it-py = "2.0.3"
loguru = "0.7.3"
markdown-it-py = "3.0.0"
mdit-py-plugins = "0.4.2"
mdurl = "0.1.2"
msgpack = "1.1.0"
multidict = "6.1.0"
nonechat = "0.3.0"
platformdirs = "4.3.6"
propcache = "0.3.0"
pycparser = "2.22"
pydantic = "2.10.6"
pydantic-core = "2.27.2"
pygments = "2.19.1"
pygtrie = "2.5.0"
python-dotenv = "1.0.1"
rich = "13.9.4"
sniffio = "1.3.1"
textual = "2.1.2"
tomli = "2.2.1"
tqdm = "4.67.1"
typing-extensions = "4.12.2"
uc-micro-py = "1.0.3"
websockets = "15.0"
win32-setctime = { version = "^1.2.0", platform = "win32" }
yarl = "1.18.3"
